<%= paginator.render do -%>
  <nav class="at-pagination" role="navigation" aria-label="pager">
    <ul>
      <%= prev_page_tag %>
      <% each_page do |page| -%>
        <% if page.display_tag? -%>
          <%= page_tag page %>
        <% elsif !page.was_truncated? -%>
          <%= gap_tag %>
        <% end -%>
      <% end -%>
      <%= next_page_tag %>
    </ul>
  </nav>
<% end -%>
