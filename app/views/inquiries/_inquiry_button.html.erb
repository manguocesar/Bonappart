<% if user_signed_in? %>
  <% if current_user.student? %>
    <%= link_to 'Contact Landlord', new_inquiry_path(apartment: params[:id]), class: 'at-btn col-sm-3 ml-2', data: { remote: true } %>
  <% end %>
<% else %>
  <%= link_to 'Contact Landlord', render_login_path, class: 'at-btn col-sm-3 ml-2', data: { remote: true } %>
<% end %>
