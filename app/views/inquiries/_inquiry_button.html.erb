<% if user_signed_in? %>
  <% if current_user.student? %>
    <%= link_to 'Contact Landlord', render_url[:url], class: 'at-btn btn-block', data: { remote: render_url[:is_remote] } %>
  <% end %>
<% else %>
  <%= link_to 'Contact Landlord', render_login_path, class: 'at-btn btn-block', data: { remote: true } %>
<% end %>
